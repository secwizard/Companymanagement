    <div class="col-md-4">
        <div class="mb-05">
            <p class="field-label-text">Address 2</p>
            <input type="text" class="textboxclass" placeholder="" id="txtAddress2">
        </div>
    </div>
    <div class="col-md-4">
        <div class="mb-05">
            <p class="field-label-text">PIN</p>
            <input type="text" class="textboxclass" placeholder="" id="txtPIN">
        </div>
    </div>
    <div class="col-md-4">
