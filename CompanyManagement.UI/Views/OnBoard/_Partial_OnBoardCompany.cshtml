@using System.Data
@model CompanyManagement.UI.Models.Response.RequestCompanyInfo


@if(Model != null)
{

}
<div class="table-responsive">
    <div class="col-md-4">
        <div class="mb-05">
            <p class="field-label-text">Company Id</p>
            <input type="text" class="textboxclass" placeholder="" id="txtCompanyId">
