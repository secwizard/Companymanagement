@using System.Data
@model CompanyManagement.UI.Models.Response.OnBoardProcessinfo


<div class="scrollsetting">
    <div class="row">
        <div class="col p-0">
            <div class="custom-table mt-3">
                <table class="table table-responsive table-bordered border-0 mb-3">
                    <thead class="custom-table-thead">
                        <tr class="text-leftt-thtd">
                            <th style="width:10%"><p class="table-blankp mb-0"></p></th>
                            <th style="width:20%"><p class="table-blankp mb-0">Subscription Name</p></th>
                            <th style="width:20%"><p class="table-blankp mb-0"> Description</p></th>
                            <th style="width:10%"><p class="table-blankp mb-0"> Product Code</p></th>
                            <th style="width:10%"><p class="table-blankp mb-0"> Metric</p></th>
                            <th style="width:20%"><p class="table-blankp mb-0"> Inclusion</p></th>
                        </tr>
                    </thead>
                    <tbody class="custom-table-tbody" id="tbl_OnBoard_Subscription_body">
                        @if (Model != null && Model.OnBoardSubscriptionInfo != null && Model.OnBoardSubscriptionInfo.Count > 0)
                        {
                            foreach (var item in Model.OnBoardSubscriptionInfo)
                            {
                                <tr id="tr_@item.SubscriptionId">
                                    <td style="width:10%">
                                        <label class="font11 all-textlabel l-p-initial cus-chkbox">
                                            <input class="permissionCheckbox" id="@item.SubscriptionId" name="PINRequired" type="checkbox" value="@item.SubscriptionId"/>
                                            <span class="checkmark"></span>
                                        </label>
                                    </td>
                                    <td style="width:20%">@item.SubscriptionName</td>
                                    <td style="width:20%">@item.Description</td>
                                    <td style="width:10%">@item.ProductCode</td>
                                    <td style="width:10%">@item.Metric</td>
                                    <td style="width:20%">@item.Inclusion</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>